{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,

  "redirects": [
    {
      "source": "/(.*)",
      "has": [{ "type": "host", "value": "www.rlranks.pro" }],
      "destination": "https://rlranks.pro/$1",
      "statusCode": 308
    }
  ],

  "rewrites": [
    { "source": "/", "destination": "/sitefrontend/index.html" },
    { "source": "/index.html", "destination": "/sitefrontend/index.html" },
    { "source": "/style.css", "destination": "/sitefrontend/style.css" },
    { "source": "/script.js", "destination": "/sitefrontend/script.js" },
    { "source": "/monetize.js", "destination": "/sitefrontend/monetize.js" },

    { "source": "/sw.js", "destination": "/sitefrontend/sw.js" },
    { "source": "/sw-register.js", "destination": "/sitefrontend/sw-register.js" },

    { "source": "/site.webmanifest", "destination": "/sitefrontend/site.webmanifest" },
    { "source": "/icons/(.*)", "destination": "/sitefrontend/icons/$1" },
    { "source": "/og-card.svg", "destination": "/sitefrontend/og-card.svg" },

    { "source": "/.well-known/assetlinks.json", "destination": "/sitefrontend/.well-known/assetlinks.json" },

    { "source": "/ranks/2v2.json", "destination": "/sitefrontend/ranks/2v2.json" }
  ],

  "headers": [
    {
      "source": "/site.webmanifest",
      "headers": [{ "key": "Content-Type", "value": "application/manifest+json; charset=utf-8" }]
    }
  ]
}