{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,

  "redirects": [
    {
      "source": "/(.*)",
      "has": [{ "type": "host", "value": "www.rlranks.pro" }],
      "destination": "https://rlranks.pro/$1",
      "statusCode": 308
    }
  ],

  "rewrites": [
    { "source": "/", "destination": "/index.html" },
    { "source": "/index.html", "destination": "/index.html" },

    { "source": "/style.css", "destination": "/style.css" },
    { "source": "/script.js", "destination": "/script.js" },
    { "source": "/monetize.js", "destination": "/monetize.js" },

    { "source": "/profile/:platform/:pid", "destination": "/api/profile.js" },

    { "source": "/api/profile", "destination": "/api/profile.js" },
    { "source": "/api/predict", "destination": "/api/predict.js" },
    { "source": "/api/health", "destination": "/api/health.js" }
  ],

  "functions": {
    "api/*.js": { "runtime": "nodejs18.x", "maxDuration": 10 }
  }
} 

